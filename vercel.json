{
    "version": 2,
    "env": {
        "HOST": "@HOST", 
        "USER1": "@USER1",
        "PORT": "3306",
        "PASSWORD": "@PASSWORD",
        "DATABASE": "@DATABASE",
        "SECRET": "@SECRET"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "env": {
                "HOST": "@HOST", 
                "USER1": "@USER1",
                "PORT": "3306",
                "PASSWORD": "@PASSWORD",
                "DATABASE": "@DATABASE",
                "SECRET": "@SECRET"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}